    <script type="text/javascript">
        //Loading page - review HIPPA checkbox status
        if ($("#hippa-checkbox").prop('checked')) {
            $('#btnSubmit').removeClass("k-state-disabled");
            $('#btnSubmit').removeProp('disabled');
        }
        else {
            $('#btnSubmit').addClass("k-state-disabled");
            $('#btnSubmit').attr('disabled', 'disabled');
        }

        $(document).ready(function () {
            $('#hippa-checkbox').click(function () {
                if (this.checked) {
                    $('#btnSubmit').removeClass("k-state-disabled");
                    $('#btnSubmit').removeProp('disabled');
                }
                else {
                    $('#btnSubmit').addClass("k-state-disabled");
                    $('#btnSubmit').attr('disabled', 'disabled');
                }
            });
        });
    </script>

